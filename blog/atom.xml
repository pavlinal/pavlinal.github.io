---
layout: default
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Paavlina Lejskova's Blog</title>
	<link href="http://plejskova.github.io/blog/atom.xml" rel="self"/>
	<link href="http://plejskova.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://plejskova.github.io/blog</id>
	<author>
		<name>Pavlina Lejskova</name>
		<email>lejsk22p@mtholyoke.edu</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://plejskova.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://plejskova.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed> 
